{
  "name": "townscup_reactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:rebuild": "cd ios && pod deintegrate && pod cache clean --all && pod update && cd .. && npm run bundle:ios",
    "ios:dev": "react-native run-ios --scheme 'TownsCup (Development)' --simulator='iPhone 14'",
    "ios:qa": "react-native run-ios --scheme 'TownsCup (QA)' --simulator='iPhone 14'",
    "start": "react-native start",
    "restart": "react-native start -- --reset-cache",
    "test": "jest --forceExit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "android:dev": " set ENVFILE=.env && react-native run-android --variant=DevelopmentDebug",
    "android:qa": "ENVFILE=.env.qa react-native run-android --variant=QaDebug",
    "android:staging": "ENVFILE=.env.staging react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "postinstall": "patch-package",
    "bundle:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "prepare": "husky install",
    "android:release:qa": "cd android && ENVFILE=.env.qa ./gradlew app:assembleRelease",
    "android:clean": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-* && rm -rf ./android/app/src/main/res/raw && cd android && ./gradlew clean",
    "nodemodule:remove": "rm -rf node_modules package-lock.json",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write",
    "set-reactnative-version": "npx rnx-align-deps --set-version[version]",
    "write-reactnative-version": "npx rnx-align-deps --requirements react-native@[version] --write"
  },
